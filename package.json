{
  "name": "@amaui/aws",
  "version": "1.0.0",
  "description": "AWS",
  "repository": "https://github.com/amaui-org/amaui-aws.git",
  "author": "Lazar <lazareric.com@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "./src/index.ts",
  "keywords": [
    "aws",
    "aws-s3",
    "aws-sdk",
    "aws-sdk-javascript",
    "backend",
    "back-end",
    "web",
    "javascript",
    "js",
    "typescript",
    "express",
    "expressjs",
    "nodejs",
    "node",
    "library",
    "amaui",
    "utils"
  ],
  "scripts": {
    "docker": "docker-compose -p 'amaui' up -d",
    "make-bucket": "cross-env AWS_ACCESS_KEY_ID=a AWS_SECRET_ACCESS_KEY=a4 aws s3 mb s3://amaui --endpoint-url http://localhost:4566",
    "make-bucket-test": "cross-env AWS_ACCESS_KEY_ID=a AWS_SECRET_ACCESS_KEY=a4 aws s3 mb s3://amaui-test --endpoint-url http://localhost:7566",
    "prebuild": "rimraf build",
    "build": "node utils/js/build.js",
    "release": "yarn test && yarn build && npm publish ./build",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "type": "tsc --noEmit -p tsconfig.json",
    "cov": "cross-env NODE_ENV=test amaui-test",
    "test:pre": "pm2 update && pm2 delete all && pm2 start utils/js/test/app.js && wait-on http://localhost:4000",
    "test:post": "pm2 delete all",
    "test": "yarn lint && yarn type && yarn build && yarn test:pre && yarn cov && yarn test:post"
  },
  "peerDependencies": {
    "aws-sdk": "^2.1054.0"
  },
  "dependencies": {
    "@amaui/date": "^1.0.114",
    "@amaui/errors": "^1.0.0",
    "@amaui/log": "^1.0.1",
    "@amaui/utils": "^1.0.174",
    "@babel/runtime": "^7.16.3"
  },
  "devDependencies": {
    "@amaui/models": "^1.0.1",
    "@amaui/test": "^1.0.147",
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.10",
    "aws-sdk": "^2.1068.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "fast-glob": "^3.2.7",
    "fs-extra": "^10.0.0",
    "playwright": "^1.17.1",
    "pm2": "^5.1.2",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.60.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.2",
    "wait-on": "^6.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=12.0.0",
    "yarn": ">=1.22"
  }
}
